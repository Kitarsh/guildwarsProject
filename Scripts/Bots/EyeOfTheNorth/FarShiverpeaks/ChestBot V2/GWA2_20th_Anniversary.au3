Func IsTyria20thAnniversaryWeapon($aItem)
    Local $lModelID = DllStructGetData($aItem, 'ModelId')   
    Local $tyriaModelIDs = [ _
        $MODEL_ID_Bone_Idol_Soul_Reaping, _
        $MODEL_ID_Bone_Idol_Blood_Magic, _
        $MODEL_ID_Bone_Idol_Curses, _
        $MODEL_ID_Bone_Idol_Death_Magic, _
        $MODEL_ID_Canthan_Targe_Strength, _
        $MODEL_ID_Canthan_Targe_Tactic, _
        $MODEL_ID_Censors_Icon_Divine_Favor, _
        $MODEL_ID_Censors_Icon_Healing_Prayers, _
        $MODEL_ID_Censors_Icon_Protection_Prayers, _
        $MODEL_ID_Censors_Icon_Smiting_Prayers, _
        $MODEL_ID_Chimeric_Prism_Fast_Casting, _
        $MODEL_ID_Chimeric_Prism_Soul_Reaping, _
        $MODEL_ID_Chimeric_Prism_Energy_Storage, _
        $MODEL_ID_Chimeric_Prism_Divine_Favor, _
        $MODEL_ID_Chimeric_Prism_Spawning_Power, _
        $MODEL_ID_Ithas_Bow, _
        $MODEL_ID_War_Pick _
    ]
    For $i = 0 To (UBound($tyriaModelIDs) -1)
		If ($lModelID == $tyriaModelIDs[$i]) Then Return True
	Next
    Return False
EndFunc

